<h1>Available Assets</h1>
<table>
    <tr>
        <th>ID</th>
        <th>Time Aquired</th>
        <th>Filename</th>
        <th>Pyramid Cache</th>
    </tr>
    {% for asset in assets %}
    <tr>
        <td>{{asset.id}}</td>
        <td>{{asset.date_added}}
        <td>{{asset.asset_filename}}</td>
        <td>{{asset.cache_filename}}</td>
    </tr>
    {% endfor %}
</table>
